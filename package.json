{
  "name": "tiny.xml",
  "version": "1.1.0",
  "description": "Tiny in-browser xml parser",
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "build": "browserify -s tinyxml index.js > dist/tinyxml.js && uglifyjs dist/tinyxml.js > dist/tinyxml.min.js"
  },
  "keywords": [
    "xml",
    "parser",
    "browser"
  ],
  "author": "Andrei Kashcha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvaka/tiny.xml"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "jasmine-core": "^4.3.0",
    "karma": "^6.4.0",
    "karma-jasmine": "^5.1.0",
    "uglify-js": "^3.17.0"
  }
}
